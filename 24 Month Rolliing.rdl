  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select Type, iMonth, iYear, [#] as App#, [$] AS Dollar$, Target, Projected, round(([$] / Target)*100,2) as PercToTargetMTD
, case when [$] = 0 then 100 else round((Projected / Target)*100,2) end as PercToTargetProjected
, day(getdate()) as DayInMonth
, DAY(EOMONTH(getdate())) as DaysInCurrentMonth
, TargetDate
from (
SELECT '1. Application Received' as Type, month(dReceived) as iMonth, YEAR(dReceived) as iYear
, count(iAppID) as [#], sum(fLoanAmount) as [$]
, (select fTargetAmt from bctblBDMTargets where cStage = 'Application' and cBDM = 'Company' and iMonth = month(ad.dReceived) and iYear = YEAR(ad.dReceived)) as Target
, DATEFROMPARTS(YEAR(dReceived), month(dReceived), 1) as TargetDate
--, case when month(getdate()) = month(dReceived) and year(getdate()) = year(dReceived) then round((cast(day(getdate()) as float) / cast(DAY(EOMONTH(getdate())) as float) ),2) * sum(fLoanAmount) else sum(fLoanAmount) end  as Projected
-- Divide amount to date for current month by number of days past
-- to get a daily rate, then multiple by days in month to get a projection
, case when month(getdate()) = month(dReceived) and year(getdate()) = year(dReceived) then (sum(fLoanAmount) / round((cast(day(getdate()) as float)),2) ) * cast(DAY(EOMONTH(getdate())) as float) else sum(fLoanAmount) end  as Projected
  FROM [BetterChoice_Central].[dbo].[bctblAppDates] ad
  where dReceived is not null
  group by year(dReceived), month(dReceived)
--   order by 3 desc,2 desc, 1
 union all
SELECT '2. Application Approved' as Type, month(dApproved) as iMonth, YEAR(dApproved) as iYear
, count(iAppID) as [#], sum(fLoanAmount) as [$]
, (select fTargetAmt from bctblBDMTargets where cStage = 'Approval' and cBDM = 'Company' and iMonth = month(ad.dApproved) and iYear = YEAR(ad.dApproved)) as Target
, DATEFROMPARTS(YEAR(dApproved), month(dApproved), 1) as TargetDate
-- Divide amount to date for current month by number of days past
-- to get a daily rate, then multiple by days in month to get a projection
, case when month(getdate()) = month(dApproved) and year(getdate()) = year(dApproved) then (sum(fLoanAmount) / round((cast(day(getdate()) as float)),2) ) * cast(DAY(EOMONTH(getdate())) as float) else sum(fLoanAmount) end  as Projected
  FROM [BetterChoice_Central].[dbo].[bctblAppDates] ad
  where dApproved is not null
  group by year(dApproved), month(dApproved)
  union all
SELECT '3. Application Declined' as Type, month(dDeclined) as iMonth, YEAR(dDeclined) as iYear
, count(iAppID) as [#], sum(fLoanAmount) as [$]
, sum(fLoanAmount) as Target
, DATEFROMPARTS(YEAR(dDeclined), month(dDeclined), 1) as TargetDate
-- Divide amount to date for current month by number of days past
-- to get a daily rate, then multiple by days in month to get a projection
, case when month(getdate()) = month(dDeclined) and year(getdate()) = year(dDeclined) then (sum(fLoanAmount) / round((cast(day(getdate()) as float)),2) ) * cast(DAY(EOMONTH(getdate())) as float) else sum(fLoanAmount) end  as Projected
  FROM [BetterChoice_Central].[dbo].[bctblAppDates] ad
  where dDeclined is not null
  group by year(dDeclined), month(dDeclined)
  union all
SELECT '4. Application Settled' as Type, month(dSettled) as iMonth, YEAR(dSettled) as iYear
, count(iAppID) as [#], sum(fLoanAmount) as [$]
, (select fTargetAmt from bctblBDMTargets where cStage = 'Settlement' and cBDM = 'Company' and iMonth = month(ad.dSettled) and iYear = YEAR(ad.dSettled)) as Target
, DATEFROMPARTS(YEAR(dSettled), month(dSettled), 1) as TargetDate
-- Divide amount to date for current month by number of days past
-- to get a daily rate, then multiple by days in month to get a projection
, case when month(getdate()) = month(dSettled) and year(getdate()) = year(dSettled) then (sum(fLoanAmount) / round((cast(day(getdate()) as float)),2) ) * cast(DAY(EOMONTH(getdate())) as float) else sum(fLoanAmount) end  as Projected
  FROM [BetterChoice_Central].[dbo].[bctblAppDates] ad
  where dSettled is not null
  group by year(dSettled), month(dSettled)
) a
where iYear between year(getdate()) - 2 and year(getdate())
  order by 3 desc,2 desc, 1</CommandText>
